#include<bits/stdc++.h>
using namespace std;
#define endl "\n"
#define mem(a,b) memset(a,b,sizeof(a))
#define sqr(a) (a*a)
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define fraction() cout.unsetf(ios::floatfield); cout.precision(10); cout.setf(ios::fixed,ios::floatfield);





int main()
{
    optimize();

    string s,t;
    int cnt=0;
    bool ans=false;
    
    cin>>s>>t;
    reverse(t.begin(), t.end());

    for(int i=0; i<s.size(); i++)
    {
        if(s[i]==t[i])
        {
            cnt++;
            if(cnt==s.size())
            {
                ans=true;
            }
        }
    }

    if(ans) cout<<"YES"<<endl;
    else cout<<"NO"<<endl;

    return 0;
}



///Easy Way:
#include<bits/stdc++.h>
using namespace std;
#define endl "\n"
#define mem(a,b) memset(a,b,sizeof(a))
#define sqr(a) (a*a)
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define fraction() cout.unsetf(ios::floatfield); cout.precision(10); cout.setf(ios::fixed,ios::floatfield);


int main()
{
    optimize();

    string s,t;
    int cnt=0;

    cin>>s>>t;
    reverse(t.begin(), t.end());

    if(s==t) cout<<"YES"<<endl;
    else cout<<"NO"<<endl;

    return 0;
}

